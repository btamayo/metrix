{
  "name": "metrix",
  "version": "1.0.0",
  "description": "The lifeblood of your product: Metadata of the data.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "npm-run-all build:client",
    "build:client": "npm-run-all build:client:transpile build:client:static",
    "build:client:transpile": "mkdir -p './tmp/client/' && browserify --transform babelify --entry './client/index.js' --outfile './tmp/client/index.js'",
    "build:client:static": "gulp build",
    "lint": "eslint './client/' './component/' './layout/' './remote/'",
    "test": "mocha --compilers js:babel-register './**/test.js'",
    "start": "npm-run-all --parallel server:postgres server:http",
    "server:postgres": "postgrest postgres://metrix:@localhost:5432/metrix --port 8081 --anonymous metrix --schema public",
    "server:http": "http-server -c-1 './tmp/'",
    "data:migrate": "knex migrate:latest",
    "data:change": "knex migrate:make",
    "data:seed": "knex seed:run"
  },
  "author": "Kurtis Rainbolt-Greene <kurtis@laurelandwolf.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurelandwolf/metrix.git"
  },
  "keywords": [
    "metrics",
    "dashboard",
    "data",
    "analysis"
  ],
  "bugs": {
    "url": "https://github.com/laurelandwolf/metrix/issues"
  },
  "homepage": "https://laurelandwolf.github.io/metrix",
  "devDependencies": {
    "babel-eslint": "6.0.2",
    "chai": "3.5.0",
    "eslint": "2.7.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-cycle": "1.0.2",
    "mocha": "2.4.5"
  },
  "dependencies": {
    "@cycle/core": "6.0.3",
    "@cycle/dom": "9.4.0",
    "@cycle/http": "8.2.2",
    "babel": "6.5.2",
    "babel-cli": "6.7.5",
    "babel-plugin-transform-es2015-arrow-functions": "6.5.2",
    "babel-plugin-transform-es2015-computed-properties": "6.6.5",
    "babel-plugin-transform-es2015-destructuring": "6.6.5",
    "babel-plugin-transform-es2015-function-name": "6.5.0",
    "babel-plugin-transform-es2015-literals": "6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.7.4",
    "babel-plugin-transform-es2015-parameters": "6.7.0",
    "babel-plugin-transform-es2015-shorthand-properties": "6.5.0",
    "babel-plugin-transform-es2015-spread": "6.6.5",
    "babel-plugin-transform-es2015-sticky-regex": "6.5.0",
    "babel-plugin-transform-es2015-typeof-symbol": "6.6.0",
    "babel-plugin-transform-es2015-unicode-regex": "6.5.0",
    "babel-plugin-transform-es3-member-expression-literals": "6.5.0",
    "babel-plugin-transform-es3-property-literals": "6.5.0",
    "babel-plugin-transform-es5-property-mutators": "6.6.5",
    "babel-plugin-transform-eval": "6.5.0",
    "babel-plugin-transform-exponentiation-operator": "6.5.0",
    "babel-plugin-transform-export-extensions": "6.5.0",
    "babel-plugin-transform-function-bind": "6.5.2",
    "babel-plugin-transform-member-expression-literals": "6.5.0",
    "babel-plugin-transform-object-assign": "6.5.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-object-set-prototype-of-to-assign": "6.6.0",
    "babel-plugin-transform-property-literals": "6.5.0",
    "babel-plugin-transform-proto-to-assign": "6.6.5",
    "babel-plugin-transform-runtime": "6.7.5",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "dotenv": "2.0.0",
    "gulp": "3.9.1",
    "gulp-cli": "1.2.1",
    "gulp-concat": "2.6.0",
    "http-server": "0.9.0",
    "knex": "0.10.0",
    "normalize.css": "4.1.1",
    "npm-run-all": "1.7.0",
    "pg": "4.5.3",
    "prettysize": "0.0.3",
    "ramda": "0.21.0",
    "require-environment-variables": "1.1.1",
    "s-ago": "1.0.6",
    "snabbdom": "0.4.2",
    "snabbdom-helpers": "3.0.0"
  }
}
